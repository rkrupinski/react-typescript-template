{
  "compilerOptions": {
    "baseUrl": ".",
    "jsx": "react",
    "noEmitOnError": true,
    "noImplicitAny": true,
    "paths": {
      "*": [
        "*",
        "src/*"
      ]
    },
    "sourceMap": true,
    "strictNullChecks": true,
    "target": "es5",
    "lib": [
      "es2015.promise",
      "dom",
      "es5",
      "scripthost"
    ]
  },
  "exclude": [
    "node_modules"
  ]
}
